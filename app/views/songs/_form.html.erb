
<%= form_for @song do |f|%>
    <%= f.label :title%>
    <p><%= f.text_field :title%></p>
    <p>Released?</p>
    <p><%=f.select :released, [['Yes', true], ['No', false]]%>
    <p><%= f.label :release_year%></p>
    <p><%= f.text_field :release_year%></p>
    <%= f.label :artist_name%>
    <p><%= f.text_field :artist_name%></p>    
    <%= f.label :genre%>
    <p><%= f.text_field :genre%></p>
<%= f.submit%>
<% end %>